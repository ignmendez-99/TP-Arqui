GLOBAL averiguo_que_syscall_es
GLOBAL averiguo_que_salida_es
GLOBAL consigo_buffer

section .text

averiguo_que_syscall_es:
  push rbp
	mov rbp, rsp

  mov rax, rax

  mov rsp, rbp
	pop rbp
	ret

averiguo_que_salida_es:
  push rbp
	mov rbp, rsp

  mov rax, rbx

  mov rsp, rbp
	pop rbp
	ret

consigo_buffer:
  push rbp
	mov rbp, rsp

  mov rax, rcx

  mov rsp, rbp
	pop rbp
	ret
